<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}


    </head>
    <body>
    <div id="base">
        {% block header %}
            {% include 'header.html.twig' %}
        {% endblock %}

        {% block notifications %}
            {% include 'notificationsSection.html.twig' %}
        {% endblock %}
        <section id="contenido">
            <div class="container">
        {% block body %}{% endblock %}
            </div>
        </section>

    </div>
    {% block footer %}
        <footer id="" class="py-3 bg-dark">
            <div class="container">
                <p class="m-0 text-center text-white"><hr/></p>
            </div>
        </footer>
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
    </body>
</html>
